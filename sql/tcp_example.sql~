
### TCP stream receives data via TCP port: 

create stream tcp_stream (
 num 	number,
 symbol string
)
using TCP(port 2333);

# create window of previous 1000 messages received from TCP
create window tcp_last_1000 
running first, last, previous, avg 
from tcp_stream.num
range 1000;

## output every message to screen
SELECT last, count 
FROM tcp_last_1000 
OUTPUT stdout ;
